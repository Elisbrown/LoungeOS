{
  "name": "loungeos",
  "version": "1.3.0",
  "description": "LoungeOS - A comprehensive management system for lounges, Snackbars, Restaurants, kitchens, and bars",
  "author": "Sunyin Elisbrown",
  "main": "electron/loader.js",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -p 9002",
    "build": "next build",
    "start": "next start -p 9002",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "db:seed": "ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/seed.ts",
    "db:init": "ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/init-db.ts",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:9002 && electron electron/main.js\"",
    "electron:build:mac": "npm run build && npm run obfuscate && npm run compile-main && electron-builder --mac --universal",
    "electron:build:win": "npm run build && npm run obfuscate && docker run --rm -v \"$(pwd)\":/project -w /project electronuserland/builder:wine bash -c 'npx bytenode --compile electron/main.js && npx electron-builder --win'",
    "electron:build:win:native": "npm run build && node scripts/prepare-standalone.js && npm run obfuscate && node scripts/compile-for-electron.js && npx electron-builder --win",
    "electron:build:all": "npm run electron:build:mac && npm run electron:build:win",
    "electron:prod": "npm run build && electron electron/main.js",
    "db:wipe": "ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/wipe-db.ts",
    "obfuscate": "node scripts/obfuscate.js",
    "rebuild": "electron-rebuild -f -w better-sqlite3",
    "compile-main": "bytenode --compile electron/main.js"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^4.1.3",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-menubar": "^1.1.6",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@types/uuid": "^10.0.0",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^11.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "embla-carousel-autoplay": "^8.1.5",
    "embla-carousel-react": "^8.1.5",
    "firebase": "^11.9.1",
    "jspdf": "^2.5.2",
    "jspdf-autotable": "^3.8.4",
    "lucide-react": "^0.460.0",
    "next": "^16.1.1-canary.3",
    "next-themes": "^0.4.6",
    "node-machine-id": "^1.1.12",
    "patch-package": "^8.0.0",
    "react": "19.0.0",
    "react-day-picker": "^8.10.1",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.54.2",
    "react-to-print": "^3.0.2",
    "recharts": "^2.13.3",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1",
    "uuid": "^11.1.0",
    "bytenode": "^1.5.7",
    "electron-is-dev": "^3.0.1",
    "electron-log": "^5.4.3",
    "electron-updater": "^6.3.9",
    "@auth/core": "^0.34.2",
    "busboy": "^1.6.0",
    "chart.js": "^4.4.6",
    "electron-serve": "^1.3.0",
    "electron-store": "^8.2.0",
    "html-pdf-node": "^1.0.8",
    "javascript-obfuscator": "^4.1.1",
    "js-cookie": "^3.0.5",
    "mysql2": "^3.11.4",
    "next-auth": "^5.0.0-beta.25",
    "nodemailer": "^7.0.0",
    "qrcode": "^1.5.4",
    "react-chartjs-2": "^5.2.0",
    "react-dropzone": "^14.3.5",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "wait-on": "^9.0.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.11",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "20.17.6",
    "@types/react": "19.0.2",
    "@types/react-dom": "19.0.2",
    "concurrently": "^9.2.1",
    "electron": "^33.0.0",
    "electron-builder": "^26.4.0",
    "javascript-obfuscator": "^5.1.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "wait-on": "^9.0.3"
  },
  "build": {
    "appId": "com.loungeos.app",
    "productName": "LoungeOS",
    "directories": {
      "output": "dist"
    },
    "files": [
      "electron/**/*",
      "!electron/main.js",
      {
        "from": ".next/standalone",
        "to": ".",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": ".next/static",
        "to": ".next/static"
      },
      {
        "from": "public",
        "to": "public"
      },
      "public/logo.png",
      "package.json",
      "template.db",
      "docs/**/*"
    ],
    "asarUnpack": [
      "**/*"
    ],
    "afterPack": "./scripts/afterPack.js",
    "mac": {
      "category": "public.app-category.business",
      "icon": "public/logo.png",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist",
      "x64ArchFiles": "**/node_modules/**/*-x64/**/*",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "universal"
          ]
        }
      ]
    },
    "win": {
      "icon": "public/logo.png",
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": false,
      "runAfterFinish": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    },
    "linux": {
      "target": [
        "deb"
      ],
      "category": "Office"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "Elisbrown",
        "repo": "LoungeOS"
      }
    ],
    "extraResources": [
      {
        "from": "resources/bin",
        "to": "bin",
        "filter": [
          "**/*"
        ]
      }
    ]
  }
}